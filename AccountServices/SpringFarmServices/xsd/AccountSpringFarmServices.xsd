						<element name="deviceNickname" type="string" minOccurs="0" maxOccurs="1"/>
						<element name="deviceStatus" type="string" minOccurs="1" maxOccurs="1"/>
						<element name="servicePlanInfo" type="Q3:ServicePlanDetailsType" minOccurs="1" maxOccurs="1"/>
						<element name="serviceEndDate" type="string" minOccurs="1" maxOccurs="1"/>
						<element name="reserveCount" type="int" minOccurs="1" maxOccurs="1"/>
						<element name="defaultDevice" type="Q1:TrueFalseStringType" minOccurs="1" maxOccurs="1"/>
						<element name="devicePartClass" type="string" minOccurs="1" maxOccurs="1"/>
						<element name="autoRefillStatus" type="Q1:TrueFalseStringType" minOccurs="1" maxOccurs="1"/>
						<element name="paymentSourceId" type="string" minOccurs="0" maxOccurs="1"/>
						<!-- WFM Start -->
						<element name="customerId" type="string" minOccurs="0" maxOccurs="1"/>
						<element name="devicePartNumber" type="string" minOccurs="0" maxOccurs="1"/>
						<element name="firstName" type="string" minOccurs="0" maxOccurs="1"/>
						<element name="lastName" type="string" minOccurs="0" maxOccurs="1"/>
						<element name="deviceModel" type="string" minOccurs="0" maxOccurs="1"/>
						<element name="manufacturer" type="string" minOccurs="0" maxOccurs="1"/>
						<!-- WFM End-->
						<element name="deviceType" type="string" minOccurs="1" maxOccurs="1"/>                                              
                                                
						<element name="reservedLine" type="boolean" minOccurs="1" maxOccurs="1"/>
						<element name="isHandsetProtectionEligible" type="boolean" minOccurs="1" maxOccurs="1"/>
						<element name="isHandsetProtectionEnrolled" type="boolean" minOccurs="1" maxOccurs="1"/>
						<element name="technology" type="string" minOccurs="1" maxOccurs="1"/>
						<element name="flashMessage" type="tns:FlashMessageType" minOccurs="0" maxOccurs="1"/>
						<element name="rewards" type="tns:RewardsType" minOccurs="0" maxOccurs="1"/>
						<!-- 
						<element name="groupId" type="string"  minOccurs="0" maxOccurs="1"/>
						<element name="groupName" type="string"  minOccurs="0" maxOccurs="1"/>
						 -->
						 <element name="group" type="tns:GroupType" minOccurs="0" maxOccurs="1"/>
						<element name="vasServices" minOccurs="0" maxOccurs="unbounded">
							<complexType>
								<sequence>
									<element name="vas" type="tns:DeviceVasInfoType" minOccurs="0" maxOccurs="1"/>
								</sequence>
							</complexType>
						</element>
						<element name="inquiryType" type="string"  minOccurs="0" maxOccurs="1"/>
						<element name="leaseStatus" type="string"  minOccurs="0" maxOccurs="1"/>
						<element name="leaseApplicationNumber" type="string"  minOccurs="0" maxOccurs="1"/>
						<element name="nextChargeDate"            type="string" minOccurs="0" maxOccurs="1"/>
						<element name="zipCode"  type="Q1:ZipCodeType"  minOccurs="0" maxOccurs="1" />
						<element name="ildEnrollStatus"  type="boolean"  minOccurs="1" maxOccurs="1" />
						<element name="ildEnrollPaymentSource"  type="string"  minOccurs="0" maxOccurs="1" />
						<element name="securityPin"  type="string"  minOccurs="0" maxOccurs="1" />
						<!-- WFM Start -->
						<element name="transactionPending"  type="string" minOccurs="0" maxOccurs="1" />
						<element name="carrierName"  type="string" minOccurs="0" maxOccurs="1" />
												<element name="simStatus"  type="string" minOccurs="0" maxOccurs="1" />
						<element name="simLegacyFlag"  type="string" minOccurs="0" maxOccurs="1" />
						<!-- WFM End-->
						<element name="payNowStatus" type="Q1:TrueFalseStringType" minOccurs="1" maxOccurs="1"/>
						<element name="isSafelink" type="Q1:TrueFalseStringType" minOccurs="1" maxOccurs="1"/>
						<element name="planPartNumber" type="string" minOccurs="1" maxOccurs="1"/>
						<element name="enrollmentPromoPrice" type="double" minOccurs="1" maxOccurs="1" />
                                                <element name="currentPlanEndDate" type="string" minOccurs="0" maxOccurs="1"/>
						<!-- IVR non-SpringFarm element Start -->
						<element name="languagePref"  type="string" minOccurs="0" maxOccurs="1" />
						<!-- IVR non-SpringFarm element End-->
					</sequence>
				</complexType>
			</element>
		</sequence>
	</complexType>
	
	
