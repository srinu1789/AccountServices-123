	<complexType name="AccountInformationDeviceTypeV2">
		<sequence>
			<element name="device" minOccurs="0" maxOccurs="unbounded">
				<complexType>
					<sequence>
						<element name="min" type="Q5:MINType" minOccurs="1" maxOccurs="1"/>
						<element name="esn" type="Q5:ESNType" minOccurs="1" maxOccurs="1"/>
